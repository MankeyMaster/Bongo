{
  "name": "bongo",
  "version": "1.0.0",
  "description": "Frontend",
  "main": "main.js",
  "scripts": {
    "bundle": "browserify --ignore-missing --no-builtins --no-commondir --insert-global-vars=\"global\" --no-browser-field main.js > main.bundle.js",
    "package-macos": "electron-packager ./ --ignore=\"(^(/main.js)$||node_modules/)\" --platform=darwin --arch=x64 --overwrite --out build/",
    "start": "electron . --disable-gpu"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MankeyMaster/Bongo.git"
  },
  "author": "Mankey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MankeyMaster/Bongo/issues"
  },
  "homepage": "https://github.com/MankeyMaster/Bongo#readme",
  "devDependencies": {
    "browserify": "^16.5.0",
    "electron": "^7.1.3",
    "electron-packager": "^14.2.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "jquery": "^3.4.1",
    "passport-spotify": "^1.1.0",
    "spotify-web-api-js": "^1.2.0"
  }
}
